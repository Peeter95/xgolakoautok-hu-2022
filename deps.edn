
{:paths ["resources" "source-code"]
 :deps  {monotech-hq/project-compiler {:git/url "git@github.com:monotech-hq/project-compiler"
                                       :sha     "1c8e1750b39f15f6c55942db7a88f3a54d8bd775"}
         monotech-hq/project-kit      {:git/url "git@github.com:monotech-hq/project-kit"
                                       :sha     "6c24958d1c602be67283256c2eb64ec82a7f4ab5"}}

 :aliases
 {:site.dev  {:exec-fn    boot-loader.backend.main/dev
              :exec-args  {:port 3000 :shadow-build :site.dev}
              :jvm-opts   ["-Dclojure.server.repl={:port,5555,:accept,clojure.core.server/repl}"]
              :extra-deps {monotech-hq/project-developer {:git/url "git@github.com:monotech-hq/project-developer.git"
                                                          :sha     "e2c23960168844bab0659dd2367d210e9bffa40b"}}}

  :app.dev    {:exec-fn    boot-loader.backend.main/dev
               :exec-args  {:port 3000 :shadow-build :app.dev}
               :jvm-opts   ["-Dclojure.server.repl={:port,5555,:accept,clojure.core.server/repl}"]
               :extra-deps {monotech-hq/project-developer {:git/url "git@github.com:monotech-hq/project-developer.git"
                                                           :sha     "e2c23960168844bab0659dd2367d210e9bffa40b"}}}

  :prod {:extra-deps {}
         :exec-fn   compiler.api/compile-app!
         :exec-args {:java-config {:aot true :jar-type :uber :main-class boot-loader.backend.main}
                     :js-builds   [:site.prod :app.prod]}}}}
